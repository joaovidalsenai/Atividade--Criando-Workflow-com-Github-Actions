{
  "name": "calculadora-ci-tests",
  "version": "1.0.0",
  "description": "Sistema de calculadora com testes automatizados",
  "main": "calculadora.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "calculadora",
    "testes",
    "ci",
    "github-actions"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "calculadora.js"
    ],
    "testMatch": [
      "**/*.test.js"
    ]
  }
}